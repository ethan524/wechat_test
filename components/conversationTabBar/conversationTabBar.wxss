.conversationTabBar{
  width: 100%;
  height:90rpx;
  background: #F3F3F3;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  position: relative;
}
.edit_view{
  display: flex;
  flex-direction: row;
}
.emoji{
  display: flex;
  width: 90rpx;
  height:90rpx;
  justify-content: center;
  align-items: center;
}
.emoji image{
  width:52rpx;
  height:52rpx;
  display: flex;
}
.input_view{
  display: flex;
  flex-direction:column;
  justify-content: center;
  width:652rpx;
  height:90rpx;
}
.user_ipt{
  display: flex;
  width:642rpx;
  height:66rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
  font-size:26rpx;
}
.upDownImg{
  width: 100%;
  height:30rpx;
  position: absolute;
  /* top:-342rpx; */
  top:-28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.upDownImg image{
  width:108rpx;
  height:30rpx;
}
