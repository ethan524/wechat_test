.robotTabBar{
  width: 100%;
  height:230rpx;
  background: #F3F3F3;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  position: relative;
}
.edit_view{
  display: flex;
  flex-direction: row;
  height: 90rpx;
}
.emoji{
  display: flex;
  width: 90rpx;
  height:90rpx;
  justify-content: center;
  align-items: center;
}
.emoji image{
  width:52rpx;
  height:52rpx;
  display: flex;
}
.input_view{
  display: flex;
  flex-direction:column;
  justify-content: center;
  width:652rpx;
  height:90rpx;
}
.user_ipt{
  display: flex;
  width:642rpx;
  height:66rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
  font-size:26rpx;
}
.btn_view{
  width: 100%;
  height:140rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.btn_view button{
  display: flex;
  justify-content: center;
  align-items: center;
  height:94rpx;
  font-size:36rpx;
  border-radius: 48rpx;
}
.update{
  color:#555555;
  width:204rpx;
  border:2rpx solid #D3D3D3;
}
.submit{
  background: #007CFF;
  color:white;
  width:460rpx;
}
