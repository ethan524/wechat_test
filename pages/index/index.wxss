.logo_robot{
  width: 100%;
  height:122px;
  display: flex;
  justify-content: center;
  margin-top:90px;
}
.logo_robot image{
  width: 122px;
  height: 122px;
}
.logo_text{
  text-align: center;
  color: white;
  font-size:14px;
  width: 100%;
  height: 30px;
  line-height: 30px;
}
.introduce{
  width: 100%;
  height: 25px;
  display: flex;
  justify-content: center;
  font-size:14px;
  color:#43979F;
  margin-top:51px;
}
.middle_line{
  margin:-1px 10px 0px;
}
.v_line{
  width:332px;
  border:1px solid;
  border-image:linear-gradient(270deg, rgba(255,255,255,0), rgba(0,231,255,1), rgba(255,255,255,0)) 1 1;
}
.verison{
  display: flex;
  width: 100%;
  height: 26px;
  justify-content: center;
  color:#0FF7EF;
  margin-top:9px;
}
.exercise_btn{
  width: 100%;
  display: flex;
  justify-content: center;
  height: 47px;
  margin-top:34px;
}
.exercise_btn image{
  width:242px;
  height: 47px;
}
.platform,.copyright{
  display: flex;
  justify-content: center;
  width: 100%;
  font-size:11px;
  color:#5D5D5D;
  margin-top: 19px;
  height:14px;
}
.copyright{
  height:10px;
  margin-top: 5px;
  font-size:10px;
  line-height: 5px;
}