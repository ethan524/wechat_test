.preview_title{
  color:#000000;
  font-size:34rpx;
  font-weight: bolder;
  width: 100%;
  height:82rpx;
  display: flex;
  align-items: center;
  text-indent: 32rpx;
}
.preview_infos{
  width: 100%;
  height:auto;
  background: #FFFFFF;
}
.preview_names{
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 40rpx;
}
.left_title{
  color:#888888;
  font-size:30rpx;
  margin-left: 40rpx;
  margin-top: 34rpx;
}
.right_info{
  margin-right:40rpx;
  margin-top: 34rpx;
}
.robot_name{
  color:#000000;
  font-size:40rpx;
}
.preview_otherinfos{
  display: flex;
  width: 100%;
  color:#888888;
  font-size:28rpx;
  margin-top:20rpx;
}
.title_text{
  margin-left: 40rpx;
  margin-right: 54rpx;
  width:15%;
}
.title_desc{
  width:65%;
  word-break: break-all;
}
.skills{
  width:65%;
  display: flex;
  flex-direction: column;
}
checkbox .wx-checkbox-input{
  border-radius: 40rpx;
  background: white;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  background: #007CFF;
  color:white;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  font-size: 22rpx;
  color: #fff;
  background: transparent;
  transform: translate(-50%, -50%) scale(1);
  -webkit-transform: translate(-50%, -50%) scale(1);
}
.skill_check{
  margin-bottom: 20rpx;
}
.preview_btns{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.preview_btns button{
  width:710rpx;
  height:94rpx;
  border-radius: 48rpx;
}
.confirm{
  background: #007CFF;
  color:white;
  margin-bottom: 32rpx;
  margin-top:40rpx;
}
.pre{
  background: #FFFFFF;
  color:#555555;
  border:2rpx solid #D3D3D3;
}