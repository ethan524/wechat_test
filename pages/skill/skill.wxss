.container{
  width : 100%;
  box-sizing: border-box;
  padding:0;
  margin: 0;
  clear: both;
}
.body-level{
  width: 100%;
  height:623px;
  background: #FAFAFA;
}
.tips-view{
  width:708rpx;
  height:312rpx;
  margin:24rpx auto 24rpx;
}
.tips-title{
  width: 100%;
  text-align: center;
  font-size:16px;
}
.tips-try{
  width: 100%;
  height:251rpx;
  margin-top:24rpx;
  border:2rpx solid #EBEBEB;
  border-radius: 22rpx;
}
.tips-image,.tips-trytext{
  float: left;
  height: 250rpx;
}
.tips-image{
  width:330rpx;
}
.tips-image image{
  width:330rpx;
  height: 250rpx;
}
.tips-trytext{
  width:378rpx;
}
