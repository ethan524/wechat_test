.skill_title{
  width: 100%;
  height:52rpx;
  display: flex;
  justify-content: center;
  font-size:32rpx;
  color:#383838;
  margin-top:20rpx;
  font-weight: bolder;
}
.skill_try{
  display: flex;
  flex-direction:row;
  width: 708rpx;
  height:250rpx;
  margin:0px auto;
  margin-top:6rpx;
  border:1px solid #DDDDDD;
  border-radius: 20rpx;
}
.tips-image{
  width:330rpx;
  height: 100%;
}
.tips-image image{
  width:330rpx;
  height:250rpx;
}
.tips-text{
  width:300rpx;
  height: 250px;
  margin-left: 36rpx;
}
.clickme-text{
  color:#606060;
  font-size:28rpx;
  margin-top:44rpx;
}
.tips-tipstext{
  margin-top: 14rpx;
  font-size:26rpx;
  color:#2EB6E1;
}
.tips-right-arrow{
  width:40rpx;
  height:250rpx;
  display: flex;
  align-items: center;
}
.tips-right-arrow image{
  width: 12rpx;
  height: 30rpx;
}
.skill_new{
  display: flex;
  align-items: center;
  justify-content: center;
  color:#383838;
  width: 100%;
  height: 52rpx;
  margin-top:30rpx;
  font-size:32rpx;
}
.skill_skills{
  width: 100%;
  height:560rpx;
  margin-top:20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.skills{
  width: 708rpx;
  height:100%;
  display: flex;
  flex-wrap:wrap;
  justify-content:space-between;
  overflow: hidden;
}
.skill_tag{
  width: 348rpx;
  height:172rpx;
  display: flex;
  flex-direction:row;
  background: #FFFFFF;
  align-items: center;
  border:1px solid #E4E4E4;
  border-radius: 10rpx;
  margin-bottom: 12rpx;
}
.no_bottom{
  margin-bottom: 0px;
}
.left_logo{
  width:140rpx;
  height:172rpx;
  text-align: center;
}
.left_logo image{
  width:82rpx;
  height:82rpx;
  margin-top:54rpx;
}
.right_intro{
  width:200rpx;
  height:172rpx;
  padding-top:28rpx;
  margin-right: 34rpx;
}
.r_title{
  color:#606060;
  font-size:28rpx;
  margin-bottom: 16rpx;
}
.r_title_intro{
  color:#0285CF;
  font-size:24rpx;
}
.skill_more{
  display: flex;
  width: 708rpx;
  height: 34rpx;
  margin: 0rpx auto;
  justify-content: flex-end;
  font-size:24rpx;
  color:#A8B0B4;
  margin-top:8rpx;
}
.hot_keywords{
  width: 100%;
  overflow: hidden;
  margin-top:32rpx;
}
.hot_keywords_title{
  width: 100%;
  height: 52rpx;
  line-height: 52rpx;
  text-align: center;
  color:#383838;
  font-weight: bolder;
  margin-bottom: 28rpx;
  font-size:32rpx;
}
.keyword_text{
  color:#157BE7;
  font-size:28rpx;
  width: 100%;
  height:52rpx;
  text-align: center;
}
.input_view{
  width: 100%;
  height:36rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom:50rpx;
  margin-top:40rpx;
}
.user_try_input{
  width: 372rpx;
  border-radius: 44rpx;
  font-size:20rpx;
  border:2rpx solid #ECECEC;
  margin: 30rpx auto 70rpx;
  background: #ECECEC;
  padding: 0rpx;
  text-align: center;
}
.skill_create{
  display: flex;
  flex-direction:row;
  width: 708rpx;
  height: 166rpx;
  margin:0rpx auto;
  margin-top:6rpx;
  border:2rpx solid #DDDDDD;
  border-radius: 20rpx;
}
.create_image{
  width:324rpx;
  height:166rpx;
}
.create_image image{
  width:100%;
  height:166rpx;
}
.create_text{
  width:300rpx;
  height:166rpx;
  text-indent: 36rpx;
  
}
.create_up_text{
  margin-top:44rpx;
  color:#606060;
  font-size:28rpx;
}
.create_down_text{
  margin-top:12rpx;
  color:#FFC02A;
  font-size:26rpx;
}
.right_share{
  text-align: center;
  width: 80rpx;
  height:166rpx;
  line-height: 166rpx;
  margin-bottom: 42rpx;
}
.right_share image{
  width: 12rpx;
  height: 30rpx;
}