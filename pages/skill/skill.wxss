.skill_title{
  width: 100%;
  height:26px;
  display: flex;
  justify-content: center;
  font-size:16px;
  color:#383838;
  margin-top:10px;
  font-weight: bolder;
}
.skill_try{
  display: flex;
  flex-direction:row;
  width: 354px;
  height: 125px;
  margin:0px auto;
  margin-top:3px;
  border:1px solid #DDDDDD;
  border-radius: 10px;
}
.tips-image{
  width:165px;
  height: 100%;
}
.tips-image image{
  width:165px;
  height: 125px;
}
.tips-text{
  width:150px;
  height: 125px;
  margin-left: 18px;
}
.clickme-text{
  font-size:14px;
  color:#606060;
  margin-top:22px;
}
.tips-tipstext{
  margin-top: 7px;
  font-size:13px;
  color:#2EB6E1;
}
.tips-right-arrow{
  width:20px;
  height:125px;
  display: flex;
  align-items: center;
}
.tips-right-arrow image{
  width: 6px;
  height: 15px;
}
.skill_new{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 26px;
  margin-top:15px;
  color:#383838;
  font-size:16px;
}
.skill_skills{
  width: 100%;
  height:280px;
  margin-top:10px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.skills{
  width: 354px;
  height:100%;
  display: flex;
  flex-wrap:wrap;
  justify-content:space-between;
  overflow: hidden;
}
.skill_tag{
  width: 174px;
  height:86px;
  display: flex;
  flex-direction:row;
  background: #FFFFFF;
  align-items: center;
  border:1px solid #E4E4E4;
  border-radius: 5px;
  margin-bottom: 6px;
}
.no_bottom{
  margin-bottom: 0px;
}

.left_logo{
  width:70px;
  height:86px;
  text-align: center;
}
.left_logo image{
  width:41px;
  height:41px;
  margin-top:27px;
}
.right_intro{
  width: 100px;
  height:86px;
  padding-top:14px;
  margin-right: 17px;
}
.r_title{
  color:#606060;
  font-size:14px;
  margin-bottom: 16rpx;
}
.r_title_intro{
  color:#0285CF;
  font-size:12px;
}
.skill_more{
  display: flex;
  width: 354px;
  height: 17px;
  margin: 0px auto;
  justify-content: flex-end;
  font-size:12px;
  color:#A8B0B4;
  margin-top:4px;
}
.hot_keywords{
  width: 100%;
  overflow: hidden;
  margin-top:16px;
}
.hot_keywords_title{
  width: 100%;
  height: 26px;
  line-height: 26px;
  text-align: center;
  color:#383838;
  font-size:16px;
  font-weight: bolder;
  margin-bottom: 14px;
}
.keyword_text{
  color:#157BE7;
  font-size:14px;
  width: 100%;
  height: 26px;
  text-align: center;
}
.input_view{
  width: 100%;
  height:18px;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom:25px;
  margin-top:20px;
}
.user_try_input{
  width: 186px;
  border-radius: 22px;
  font-size:12px;
  border:2rpx solid #ECECEC;
  margin: 15px auto 35px;
  background: #ECECEC;
  padding: 0px;
  text-align: center;
}
.skill_create{
  display: flex;
  flex-direction:row;
  width: 354px;
  height: 83px;
  margin:0px auto;
  margin-top:3px;
  border:1px solid #DDDDDD;
  border-radius: 10px;
}
.create_image{
  width:162px;
  height:83px;
}
.create_image image{
  width:100%;
  height:83px;
}
.create_text{
  width:150px;
  height:83px;
  text-indent: 18px;
  
}
.create_up_text{
  margin-top:22px;
  color:#606060;
  font-size:14px;
}
.create_down_text{
  margin-top:6px;
  color:#FFC02A;
  font-size:13px;
}
.right_share{
  width: 40px;
  height:83px;
  text-align: center;
  line-height: 83px;
  margin-bottom: 21px;
}
.right_share image{
  width: 6px;
  height: 15px;
}